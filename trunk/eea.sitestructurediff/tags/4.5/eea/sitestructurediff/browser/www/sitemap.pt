	<title i18n:translate="">Site visualisation</title>

	<link rel="stylesheet" type="text/css" href="" tal:attributes="href string:$portal_url/++resource++jstree/source/tree_component.css" />
	<script type="text/javascript" src="" tal:attributes="src string:$portal_url/++resource++jstree-lib.js"></script>
	<script type="text/javascript" src="" tal:attributes="src string:$portal_url/++resource++jstree/source/tree_component.min.js"></script>
